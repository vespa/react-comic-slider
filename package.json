{
  "name": "react-comic-slider",
  "version": "1.0.0",
  "description": "just a slide to see comic strips",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "updateSnapshot": "jest --updateSnapshot",
    "start": "webpack-dev-server --open",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vespa/react-comic-slider.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vespa/react-comic-slider/issues"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/js/components/**/**.{js,jsx}"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^container/(.*)": "<rootDir>/src/js/components/container/$1",
      "^presentational/(.*)": "<rootDir>/src/js/components/presentational/$1",
      "^config/(.*)": "<rootDir>/src/js/components/config/$1",
      "^libs/(.*)": "<rootDir>/src/js/libs/$1"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.0.5",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "homepage": "https://github.com/vespa/react-comic-slider#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.2",
    "isomorphic-fetch": "^2.2.1"
  }
}
